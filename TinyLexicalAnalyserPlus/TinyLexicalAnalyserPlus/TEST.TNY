{ Sample program
  in TINY language -
  computes factorial
}
/* This is test comment
   This is test comment */
read x; { input an integer }
if 0.1 < x then { don't compute if x <= 0 }
  fact := 1;
  repeat
    fact := fact * x;
    x := x - 1
  until x = 0;
  write fact  { output factorial of x }
end
